{
  "name": "procatty-api",
  "version": "1.0.0",
  "main": "src/.dist/server.js",
  "license": "MIT",
  "description": "catalog professional application",
  "repository": "https://bitbucket.org/melk_de_sousa/procatty",
  "author": "Melk de Sousa",
  "private": true,
  "scripts": {
    "start": "node ./dist/src/server.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "predev": "yarn prisma generate && yarn prisma db push",
    "dev": "nodemon -w src/ -e ts,js,json -i node_modules/ -x ts-node src/app/server.ts",
    "prisma": "npx prisma"
  },
  "dependencies": {
    "@prisma/client": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "nodemon": "^2.0.12",
    "prisma": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}
